#include <stdio.h>

int main() {

    int num = 1;

    do{
            if (num ==6 || num==8){
                num++;
                continue; // this ignores the following line if this condition is met i.e. once number 6 and 8 were picked it skipped the print function
            }

        printf("%d is available \n",num);
        num++;
    }while(num<=10);

    return 0;
}
