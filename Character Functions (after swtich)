#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>
#include <string.h>
#include <math.h>

int main() {
    int tuna = 'Â£';

    if (isalpha(tuna)) { // Check if tuna is a letter
        printf("%c is a letter\n", tuna);
    }else{

        if( isdigit(tuna)){ // Check if tuna is a digit
            printf("%c is a number", tuna);
        }else{
            printf("%c is a WHAT IS THAT?", tuna);
        }

    }

    return 0;
}
