#include <stdio.h>
#include <stdlib.h>

int main() {

    int i;
    for(i = 1; i <= 5; i++) {
        printf("%d\n", i);
    }

    int luckyNumbers[] = {4, 8, 15, 16, 23, 42};

    // Reuse the variable 'i' without redeclaration
    for(i = 0; i < 6; i++) { // Corrected loop condition to avoid going out of bounds
        printf("%d\n", luckyNumbers[i]);
    }

    return 0;
}
